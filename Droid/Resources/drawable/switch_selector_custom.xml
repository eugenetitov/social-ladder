<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
  <item android:state_checked="true">
    <shape
        android:shape="rectangle"
        android:visible="true"
        android:dither="true"
        android:useLevel="false">
      <solid android:color="#fff"/>
      <stroke android:width="1dp" android:color="@drawable/switch_selector_color"/>
      <corners
          android:radius="15dp"/>
      <size
          android:width="28dp"
          android:height="28dp" />
    </shape>
  </item>
  <item android:state_checked="false">
    <shape
        android:shape="rectangle"
        android:visible="true"
        android:dither="true"
        android:useLevel="false">
      <solid android:color="#fff"/>
      <stroke android:width="1dp" android:color="@drawable/switch_selector_color"/>
      <corners
          android:radius="15dp"/>
      <size
          android:width="28dp"
          android:height="28dp" />
    </shape>
  </item>
</selector>